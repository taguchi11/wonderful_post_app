<h1>マイページ</h1>
<%= link_to '記事作成', "/articles/new" %>

<table>
  <tr>
    <th>タイトル</th>
    <th>内容</th>
  </tr>
  <% @articles.each do |article| %>
  <tr>
  <td><%= article.title %></td>
  <td><%= article.content %></td>
  <td><%= link_to '詳細', "/articles/#{article.id}" %></td>
  </tr>
  <% end %>
</table>
